<Bindings>
  <Binding name="MONEY_MAKING_ASSISTANT_CONFIRM_BUTTON" runOnUp="true" category="ADDONS" header="MONEY_MAKING_ASSISTANT">
    if keystate == 'down' then
      MoneyMakingAssistant.confirm()
    end
  </Binding>
  <Binding name="MONEY_MAKING_ASSISTANT_STOP" runOnUp="true" category="ADDONS">
    if keystate == 'down' then
      MoneyMakingAssistant.stop()
    end
  </Binding>
</Bindings>
